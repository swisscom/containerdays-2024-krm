---
apiVersion: netbox.dev/v1
kind: PrefixClaim
metadata:
  name: "{{ input.resources[0].metadata.name }}"
  namespace: "{{ input.resources[0].metadata.namespace }}"
  ownerReferences:
    - apiVersion: {{ input.resources[0].apiVersion }}
      kind: {{ input.resources[0].kind }}
      name: {{ input.resources[0].metadata.name }}
      uid: {{ input.resources[0].metadata.uid }}
spec:
  parentPrefix: "{{ input.resources[0].data.parentPrefix }}"
  prefixLength: "{{ input.resources[0].data.prefixLength }}"
  preserveInNetbox: true
  description: "Automatically generated by Ansible"
